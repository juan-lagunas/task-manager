{
  "schema": "https://railway.app/railway.schema.json",
  "build": {
      "builder": "NIXPACKS"
  },
  "deploy": {
      "startCommand": "python manage.py migrate && python manage.py collectstatic --noinput && gunicorn tasks.wsgi",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
  },
  "devDependencies": {
    "tailwind-scrollbar": "^3.0.4"
  }
}